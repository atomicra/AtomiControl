    "url": "https://github.com/atomicra/AtomiControl/issues"
  },
  "homepage": "https://github.com/atomicra/AtomiControl#readme",
  "devDependencies": {
    "stats.js": "^0.17.0",
    "three": "^0.151.3",
    "rollup": "^2.3.2",
    "google-closure-compiler": "20200224.0.0"    
  }
